# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 1 <vox>."

function run {
    local -r word="$1"
    local -r stem="${word%a}"

    echo "${stem}a"
    echo "${stem}a"
    echo "${stem}am"
    echo "${stem}ae"
    echo "${stem}ae"
    echo "${stem}ā"
    echo "${stem}ae"
    echo "${stem}ae"
    echo "${stem}ās"
    echo "${stem}ārum"
    echo "${stem}īs"
    echo "${stem}īs"

    echo "1"
}
