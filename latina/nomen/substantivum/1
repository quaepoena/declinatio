# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 1 <vox>."

function run {
    local -r vox="$1"
    local -r radix="${vox%a}"

    echo "${radix}a"
    echo "${radix}a"
    echo "${radix}am"
    echo "${radix}ae"
    echo "${radix}ae"
    echo "${radix}ā"
    echo "${radix}ae"
    echo "${radix}ae"
    echo "${radix}ās"
    echo "${radix}ārum"
    echo "${radix}īs"
    echo "${radix}īs"

    echo "1"
}
