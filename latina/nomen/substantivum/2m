# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 2m <vox>"

function run {
    local -r word="$1"
    local -r stem="${word%us}"

    echo "${stem}us"
    echo "${stem}e"
    echo "${stem}um"
    echo "${stem}ī"
    echo "${stem}ō"
    echo "${stem}ō"
    echo "${stem}ī"
    echo "${stem}ī"
    echo "${stem}ōs"
    echo "${stem}ōrum"
    echo "${stem}īs"
    echo "${stem}īs"

    echo "2m"
}
