# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 2m <vox>."

function run {
    local -r word="$1"
    local -r stem="${word%us}"

    echo "${stem}us"
    echo "${stem}e"
    echo "${stem}um"
    echo "${stem}i"
    echo "${stem}o"
    echo "${stem}o"
    echo "${stem}i"
    echo "${stem}i"
    echo "${stem}os"
    echo "${stem}orum"
    echo "${stem}is"
    echo "${stem}is"

    echo "2m"
}
