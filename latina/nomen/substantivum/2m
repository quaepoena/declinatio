# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 2m <vox>"

function run {
    local -r vox="$1"
    local -r radix="${vox%us}"

    echo "${radix}us"
    echo "${radix}e"
    echo "${radix}um"
    echo "${radix}ī"
    echo "${radix}ō"
    echo "${radix}ō"
    echo "${radix}ī"
    echo "${radix}ī"
    echo "${radix}ōs"
    echo "${radix}ōrum"
    echo "${radix}īs"
    echo "${radix}īs"

    echo "2m"
}
