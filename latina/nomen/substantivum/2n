# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 2n <vox>"

function run {
    local -r word="$1"
    local -r stem="${word%um}"

    echo "${stem}um"
    echo "${stem}um"
    echo "${stem}um"
    echo "${stem}ī"
    echo "${stem}ō"
    echo "${stem}ō"
    echo "${stem}a"
    echo "${stem}a"
    echo "${stem}a"
    echo "${stem}ōrum"
    echo "${stem}īs"
    echo "${stem}īs"

    echo "2n"
}
