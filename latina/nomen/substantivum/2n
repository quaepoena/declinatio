# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 2n <vox>"

function run {
    local -r vox="$1"
    local -r radix="${vox%um}"

    echo "${radix}um"
    echo "${radix}um"
    echo "${radix}um"
    echo "${radix}ī"
    echo "${radix}ō"
    echo "${radix}ō"
    echo "${radix}a"
    echo "${radix}a"
    echo "${radix}a"
    echo "${radix}ōrum"
    echo "${radix}īs"
    echo "${radix}īs"

    echo "2n"
}
