# ; -*- mode: Shell-Script;-*-

argument_count=2
usage="Usus: 3-ium <vox> <radix>"

function run {
    local -r vox="$1"
    local -r radix="$2"

    echo "${vox}"
    echo "${vox}"
    echo "${radix}em"
    echo "${radix}is"
    echo "${radix}i"
    echo "${radix}e"
    echo "${radix}es"
    echo "${radix}es"
    echo "${radix}es"
    echo "${radix}ium"
    echo "${radix}ibus"
    echo "${radix}ibus"

    echo "3-ium"
}
