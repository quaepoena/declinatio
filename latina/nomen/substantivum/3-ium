# ; -*- mode: Shell-Script;-*-

argument_count=2
usage="Usus: 3f-ium <vox> <radix>"

function run {
    local -r word="$1"
    local -r stem="$2"

    echo "${word}"
    echo "${word}"
    echo "${stem}em"
    echo "${stem}is"
    echo "${stem}i"
    echo "${stem}e"
    echo "${stem}es"
    echo "${stem}es"
    echo "${stem}es"
    echo "${stem}ium"
    echo "${stem}ibus"
    echo "${stem}ibus"

    echo "3f-ium"
}
