# ; -*- mode: Shell-Script;-*-

argument_count=2
usage="Usus: 3-um <vox> <radix>."

function run {
    local -r vox="$1"
    local -r radix="$2"

    echo "${vox}"
    echo "${vox}"
    echo "${radix}em"
    echo "${radix}is"
    echo "${radix}ī"
    echo "${radix}e"
    echo "${radix}ēs"
    echo "${radix}ēs"
    echo "${radix}ēs"
    echo "${radix}um"
    echo "${radix}ibus"
    echo "${radix}ibus"

    echo "3-um"
}
