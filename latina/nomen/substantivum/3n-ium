# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 3n-ium <vox> <radix>"

function run {
    local -r vox="$1"
    local -r radix="$2"

    echo "${vox}"
    echo "${vox}"
    echo "${vox}"
    echo "${radix}is"
    echo "${radix}ī"
    echo "${radix}ī"
    echo "${radix}ia"
    echo "${radix}ia"
    echo "${radix}ia"
    echo "${radix}ium"
    echo "${radix}ibus"
    echo "${radix}ibus"

    echo "3n-ium"
}
