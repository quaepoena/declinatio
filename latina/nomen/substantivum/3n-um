# ; -*- mode: Shell-Script;-*-

argument_count=2
usage="Usus: 3n-um <vox> <radix>"

function run {
    local -r vox="$1"
    local -r radix="$2"

    echo "${vox}"
    echo "${vox}"
    echo "${vox}"
    echo "${radix}is"
    echo "${radix}Ä«"
    echo "${radix}e"
    echo "${radix}a"
    echo "${radix}a"
    echo "${radix}a"
    echo "${radix}um"
    echo "${radix}ibus"
    echo "${radix}ibus"

    echo "3n-um"
}
