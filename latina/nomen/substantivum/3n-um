# ; -*- mode: Shell-Script;-*-

argument_count=2
usage="Usage: 3n-um word stem."

function run {
    local -r word="$1"
    local -r stem="$2"

    echo "${word}"
    echo "${word}"
    echo "${word}"
    echo "${stem}is"
    echo "${stem}i"
    echo "${stem}e"
    echo "${stem}a"
    echo "${stem}a"
    echo "${stem}a"
    echo "${stem}um"
    echo "${stem}ibus"
    echo "${stem}ibus"

    echo "3n-um"
}
