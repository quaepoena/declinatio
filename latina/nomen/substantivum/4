# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 4 <vox>"

function run {
    local -r vox="$1"
    local -r radix="${vox%us}"

    echo "${radix}us"
    echo "${radix}us"
    echo "${radix}um"
    echo "${radix}ūs"
    echo "${radix}uī"
    echo "${radix}ū"
    echo "${radix}ūs"
    echo "${radix}ūs"
    echo "${radix}ūs"
    echo "${radix}uum"
    echo "${radix}ibus"
    echo "${radix}ibus"

    echo "4"
}
