# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usage: 4 word."

function run {
    local -r word="$1"
    local -r stem="${word%us}"

    echo "${stem}us"
    echo "${stem}us"
    echo "${stem}um"
    echo "${stem}us"
    echo "${stem}ui"
    echo "${stem}u"
    echo "${stem}us"
    echo "${stem}us"
    echo "${stem}us"
    echo "${stem}uum"
    echo "${stem}ibus"
    echo "${stem}ibus"

    echo "4"
}
