# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 4n <vox>"

function run {
    local -r vox="$1"
    local -r radix="${vox%u}"

    echo "${radix}u"
    echo "${radix}u"
    echo "${radix}u"
    echo "${radix}큰s"
    echo "${radix}큰/u카"
    echo "${radix}큰"
    echo "${radix}ua"
    echo "${radix}ua"
    echo "${radix}ua"
    echo "${radix}uum"
    echo "${radix}ibus"
    echo "${radix}ibus"

    echo "4n"
}
