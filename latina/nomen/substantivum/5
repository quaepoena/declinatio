# ; -*- mode: Shell-Script;-*-

argument_count=1
usage="Usus: 5 <vox>"

function run {
    local -r vox="$1"
    local -r radix="${vox%s}"

    echo "${radix}s"
    echo "${radix}s"
    echo "${radix}m"
    echo "${radix}ī"
    echo "${radix}ī"
    echo "${radix}"
    echo "${radix}s"
    echo "${radix}s"
    echo "${radix}s"
    echo "${radix}rum"
    echo "${radix}bus"
    echo "${radix}bus"

    echo "5"
}
